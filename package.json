    "npm-run-all": "^4.1.3",
    "react": "^16.13.1",
    "react-addons-update": "^15.6.2",
    "react-animated-css": "^1.0.4",
    "react-apexcharts": "^1.3.6",
    "react-d3-legends": "0.0.19",
    "react-dom": "^16.4.0",
    "react-on-screen": "^2.1.1",
